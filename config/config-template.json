{
  "prefix": "<YOUR_PREFIX>",
  "client_id": "<YOUR_CLIENT_ID>",
  "token": "<YOUR_TOKEN>",
  "enable_prefix_change": true,

  "enable_logging": true,
  "log_file_name": "combined.log",

  "role_ids_admin": [ "" ],
  "user_ids_admin": [ "" ],

  "enable_activity": true,
  "activity": {
    "name": "<prefix>help",
    "type": "PLAYING"
  },

  "help": {
    "show_only_permitted_commands": false,
    "send_to_dm": false,
    "show_cmd_modifications": true
  },

  "embed": {
    "color": "RANDOM",
    "author_name": "",
    "avatar_url": ""
  },

  "default_lang": "en",
  "enable_lang_change": true,
  "lang_paths": {
    "de": "text_de.json",
    "en": "text_en.json"
  },

  "enable_standard_commands": true,
  "enable_slash_commands": true,
  "auto_slash_options": false
}
